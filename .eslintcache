assName=\"row\">\n                    <NameFilter\n\n                        handleInputChange={this.handleInputChange}\n                        searcher={this.state.search}\n                        resulter={this.state.result}\n                    >\n                    </NameFilter>\n                </div>\n                <div className=\"row\">\n                    <div className=\"col col-md-6\">\n                        <h3>Name</h3>\n                    </div>\n                    <div className=\"col col-md-6\">\n                        <button onClick={this.sortEmp} className=\"btn btn-success\">Age (click to sort)</button>\n                    </div>\n                </div>\n\n                <EmpName\n                    theName={this.state.result}>\n                </EmpName>\n            </div>\n        );\n    }\n}\n\nexport default EmpContainer;\n","C:\\Users\\Laurie\\gitmt\\Homework\\19_Employee_Directory\\src\\components\\EmpName.js",[],"C:\\Users\\Laurie\\gitmt\\Homework\\19_Employee_Directory\\src\\components\\NameFilter.js",[],"C:\\Users\\Laurie\\gitmt\\Homework\\19_Employee_Directory\\src\\utils\\API.js",["32"],"import axios from \"axios\";\nconst BASEURL = \"https://randomuser.me/api/?results=50&seed=foobar\";\n\nexport default {\n  search: function(query) {\n    return axios.get(BASEURL);\n  }\n};\n",{"ruleId":"33","replacedBy":"34"},{"ruleId":"35","replacedBy":"36"},{"ruleId":"37","severity":1,"message":"38","line":16,"column":15,"nodeType":"39","messageId":"40","endLine":16,"endColumn":19},{"ruleId":"41","severity":1,"message":"42","line":4,"column":1,"nodeType":"43","endLine":8,"endColumn":3},"no-native-reassign",["44"],"no-negated-in-lhs",["45"],"no-unused-vars","'emps' is assigned a value but never used.","Identifier","unusedVar","import/no-anonymous-default-export","Assign object to a variable before exporting as module default","ExportDefaultDeclaration","no-global-assign","no-unsafe-negation"]